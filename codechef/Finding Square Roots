// SOLUTION -

#include <iostream>
#include <math.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	int x;
	cin >> t;
	while(t--){
	    cin >> x;
	    cout << int(sqrt(x)) << endl;
	}
	return 0;
}
