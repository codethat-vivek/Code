// Question Link - https://www.codechef.com/problems/FLOW007

// Solution Link - https://www.codechef.com/viewsolution/36497127

// SOLUTION -

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	int n;
	while(t--){
	    cin >> n;
	    int temp = n;
	    int r = 0;
	    
	    while(temp > 0){
	        r = 10*r + (temp % 10);
	        temp = temp / 10;
	    }
	    cout << r << endl;
	}
	return 0;
}
